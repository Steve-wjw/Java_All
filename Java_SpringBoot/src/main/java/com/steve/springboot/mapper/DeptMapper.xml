<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.steve.springboot.mapper.DeptMapper">
    <select id="getDepts" resultType="com.steve.springboot.pojo.Dept">
        select * from dept;
    </select>

    <select id="getDept" resultType="com.steve.springboot.pojo.Dept">
        select * from dept where deptNo = #{deptNo};
    </select>
</mapper>